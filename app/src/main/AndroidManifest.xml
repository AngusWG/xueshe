<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.huas.xueshe">

    <!-- 添加访问网络的权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.Test" />
        <activity android:name=".notUse.MyListView4" />
        <!--
ATTENTION: This was auto-generated to add Google Play services to your project for
 App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".activity.UserInfo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.activity.activityList"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_activity"
            android:theme="@style/FullscreenTheme" />
        <activity android:name=".activity.activity.activityInfo" />
        <activity android:name=".activity.comment" />
        <activity
            android:name=".activity.activity.creatActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_creat"
            android:theme="@style/FullscreenTheme"></activity>
    </application>

</manifest>